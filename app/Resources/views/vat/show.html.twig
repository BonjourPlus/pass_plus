{% extends '@PassPlus/Admin/adminLayout.html.twig' %}

{% block content %}
    <h1>Vat</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vat.id }}</td>
            </tr>
            <tr>
                <th>Rate</th>
                <td>{{ vat.rate }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vat_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('vat_edit', { 'id': vat.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
